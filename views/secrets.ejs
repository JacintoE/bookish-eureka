<%- include ("partials/header.ejs")-%>

<div class="jumbotron text-center">
  <div class="container">
    <i class="fa fa-key fa-6x"></i>
    <h1 class="display-3">You've discovered my secrets!</h1>

    <% usersWithSecrets.forEach(function(user){ %>
      <p class="secret-text"><%= user.secret %></p>
    <% }); %>


    <hr>
    <a class="btn btn-light btn-lg" href="/logout">Logout</a>
    <a class="btn btn-dark btn-lg" href="/submit">Submit a secret</a>

  </div>

</div>


